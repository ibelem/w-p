{
  "name": "webnizer-portal",
  "version": "0.0.1",
  "private": false,
  "description": "Webnizer Portal",
  "scripts": {
    "dev": "concurrently \"node server/server.js\" \"vite --host\"",
    "build": "vite build",
    "build-tsc": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "prepare": "husky install",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@/github.com/ibelem/webnizer-portal.git"
  },
  "keywords": [
    "Vite",
    "Vue3",
    "TypeScript",
    "Webnizer"
  ],
  "author": "Belem",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ibelem/webnizer-portal/issues"
  },
  "homepage": "https://github.com/ibelem/webnizer-portal#readme",
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "element-plus": "^2.1.11",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.4.0",
    "vite": "^2.9.6",
    "vite-plugin-element-plus": "0.0.12",
    "vite-plugin-monaco-editor-nls": "^1.0.8",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vue/vue3-jest": "^27.0.0",
    "commitizen": "^4.2.4",
    "concurrently": "^7.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "jest": "^28.0.3",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "stylus": "^0.57.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.4",
    "vue-tsc": "^0.34.11"
  }
}
